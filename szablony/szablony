####################### FUEL MECHANIC TEST
[event]
        name=new turn
        first_time_only=yes
        id=FUEL
        [modify_unit]
            [filter]
                variable=FUEL
            [/filter]
            [set_variable]
                name=FUEL
                value={CAPICY}
                id=FUEL
            [/set_variable]
        [/modify_unit]
[/event]
[event]
        name=new turn
        first_time_only=no
        id=FUEL
        [modify_unit]
            [filter]
                variable=FUEL
            [/filter]
            [set_variable]
                name = FUEL
                sub = 1
            [/set_variable]
        [/modify_unit]
        [if]
        
            [have_unit]
                [filter]
                    variable=FUEL
                [/filter]
            [/have_unit]
            [then]
            
                [modify_unit]
                    [filter]
                        variable=FUEL
                    [/filter]
                    kill=yes
                [/modify_unit]
            
            [/then]
         
        [/if]
[/event]

